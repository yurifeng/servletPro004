<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <context-param>
        <param-name>name</param-name>
        <param-value>jsd0x001</param-value>
    </context-param>
    <context-param>
        <param-name>pwd</param-name>
        <param-value>0x0000aaaabbbbb</param-value>
    </context-param>


    <!-- <servlet>
         <servlet-name>RequestServlet</servlet-name>
         <servlet-class>com.yuri.servletPro.servlet.RequestServlet</servlet-class>
     </servlet>
     <servlet>
         <servlet-name>ResponseServlet</servlet-name>
         <servlet-class>com.yuri.servletPro.servlet.ResponseServlet</servlet-class>
     </servlet>-->
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.MainServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CookieServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.CookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCookieServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.GetCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckCookieServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.CheckCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.Session.SessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSessionServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.Session.GetSessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryInfoServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.QueryInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletContextServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.ServletContextServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetServletContextServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.GetServletContextServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitialServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.InitialServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ServletConfigServlet</servlet-name>
        <servlet-class>com.yuri.servletPro.servlet.ServletConfigServlet</servlet-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>utf-8</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletConfigServlet</servlet-name>
        <url-pattern>/sconfig</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitialServlet</servlet-name>
        <url-pattern>/count</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetServletContextServlet</servlet-name>
        <url-pattern>/gscs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletContextServlet</servlet-name>
        <url-pattern>/scs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryInfoServlet</servlet-name>
        <url-pattern>/cx</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSessionServlet</servlet-name>
        <url-pattern>/getSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionServlet</servlet-name>
        <url-pattern>/ss</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckCookieServlet</servlet-name>
        <url-pattern>/checkCk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCookieServlet</servlet-name>
        <url-pattern>/getCk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CookieServlet</servlet-name>
        <url-pattern>/ck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
